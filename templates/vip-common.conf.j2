# Common VIP settings which can be overridden in individual vip-<nnnn>.conf
PASSWORD="{{ucarp_pw}}"
BIND_INTERFACE="{{ucarp_if}}"
SOURCE_ADDRESS="{{ucarp_srcaddr}}"

# If you have extra options to add, see "ucarp --help" output
OPTIONS="{% for item in ucarp_options %}--{{item}}{% if not loop.last %} {% endif %}{% endfor %}"

